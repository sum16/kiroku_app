<!DOCTYPE html>
<html>
  <head>
    <title>Kiroku</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>


    <%= stylesheet_link_tag "staff", media: "all", 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

     <body>
    <div id="wrapper">
      <%= render "shared/header" %>
    </div>

    <div id="container-field">
        <div id="container">
          <%= yield %>
        </div>
      <%= render "shared/footer" %>
    </div>
  
  </body>
</html>
