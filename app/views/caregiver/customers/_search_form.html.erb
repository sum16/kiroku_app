   <%= form_with model: @search_form, scope: "search", url: :caregiver_staff_member_customers, html: { method: :get, class: "search"} do |f| %>
    <div class="search-form">
      <%= f.label :family_name, "名字" %>
      <%= f.text_field :family_name %>
    </div>

     <div class="search-form">
      <%= f.label :given_name, "名前" %>
      <%= f.text_field :given_name %>
    </div>

     <div class="search-form">
      <%= f.label :family_name_kana, "名字(カナ)" %>
      <%= f.text_field :family_name_kana %>
    </div>

     <div class="search-form">
      <%= f.label :given_name_kana, "名前(カナ)" %>
      <%= f.text_field :given_name_kana %>
    </div>

    <div class="search-form">
      <%= f.select :gender, [ [ "男性", "male"], [ "女性", "female" ]], {include_blank: '性別を選択'}, 
      class: 'sample' %>
    </div>

    <%= f.submit "検索" %>


  <% end %>
